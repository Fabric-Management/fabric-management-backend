    {
      "name": "üßµ Production - Fibers",
      "item": [
        {
          "name": "Get Default System Fibers (100% Pure)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.data && jsonData.data.length > 0) {",
                  "    // Find Viscose fiber (CV code)",
                  "    var cotton = jsonData.data.find(f => f.fiberName.includes('Cotton'));",
                  "    var viscose = jsonData.data.find(f => f.fiberName.includes('Viscose'));",
                  "    if (cotton) {",
                  "        pm.environment.set('fiber_id_cotton', cotton.id);",
                  "        console.log('‚úÖ Cotton fiber ID:', cotton.id);",
                  "    }",
                  "    if (viscose) {",
                  "        pm.environment.set('fiber_id_viscose', viscose.id);",
                  "        pm.environment.set('material_id_viscose', viscose.materialId);",
                  "        console.log('‚úÖ Viscose fiber ID:', viscose.id);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/production/fibers",
              "host": ["{{base_url}}"],
              "path": ["api", "production", "fibers"]
            }
          }
